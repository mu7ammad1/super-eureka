{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/SignInWith0AuthButton.tsx"
, "app/(create)/tools/CustomZoomDiv.jsx", "app/(create)/ii/initial-elements.jsx", "app/(create)/ii/CircleNode.jsx", "app/(create)/ii/page.jsx", "app/(create)/ii/ToolbarNode.jsx", "app/(create)/ii/TextInputNode.jsx", "app/(create)/ii/ResizerNode.jsx", "app/(create)/ii/ButtonEdge.jsx"  ],
  "exclude": ["node_modules"]
}
